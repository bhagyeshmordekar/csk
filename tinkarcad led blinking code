void setup() {
// put your setup code here, to run once:
pinMode(7, OUTPUT);// FEEDER
pinMode(8, OUTPUT);// PUNCHING MACHINE
}
void loop() {
// put your main code here, to run repeatedly:
digitalWrite(7,HIGH);
delay(1000);
digitalWrite(8,HIGH);
delay(1000);
digitalWrite(8,LOW);
delay(1000);
digitalWrite(8,HIGH);
delay(1000);
digitalWrite(8,LOW);
delay(1000);
digitalWrite(8,HIGH);
delay(1000);
digitalWrite(8,LOW);
digitalWrite(7,LOW);
delay(1000);
}
